{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "ConsoleApp": "Data Source=localhost;Initial Catalog=ConsoleApp;Integrated Security=True;TrustServerCertificate=True;"
    },
    "AppSettings": {
        "SqliteDatabaseName": "ConsoleApp",
        "DatabaseProvider": "Sqlite" // Can be set to "Sqlite" or "SqlServer"
    }
}